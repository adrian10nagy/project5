<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <!-- header_top -->
    <div class="top_bg">
        <div class="container">
            <div class="header_top">
                @*<div class="top_left">
                          <h2><a href="#">50%off</a> use coupon code "big61" and get extra 33% off on orders above rs 2,229 </h2>
                    </div>*@
                <div class="top_left">
                    <div class="logo">
                        <a href="index.html"> Antreprenor </a>
                    </div>
                </div>
                <div class="top_right">
                    <ul class="nav navbar-nav navbar-right">
                        <li class="login">
                            @Html.Partial("~/Views/Shared/_Account.cshtml")
                        </li>
                        <li>|</li>
                        <li> @Html.ActionLink(" + ADAUGA ANUNT NOU", "Add", "Offer") </li>
                    </ul>
                </div>
                <div class="clearfix"> </div>
            </div>
        </div>
    </div>
    <!-- header -->
    <div class="header_bg">
        <div class="container">
            <div class="header">

                <!-- start header_right -->
                <div class="header_right">
                    <div class="create_btn">
                        <a class="arrow" href="registration.html">creaaza-ti cont >  </a>
                    </div>
                    <ul class="icon1 sub-icon1 profile_img">
                        <li>
                            <a class="active-icon c2" href="#"> </a>
                            <ul class="sub-icon1 list">
                                <li><h3>shopping cart empty</h3><a href=""></a></li>
                                <li><p>if items in your wishlit are missing, <a href="">login to your account</a> to view them</p></li>
                            </ul>
                        </li>
                    </ul>
                    <ul class="icon1 sub-icon1 profile_img">
                        <li>
                            <a class="active-icon c1" href="#"> </a>
                            <ul class="sub-icon1 list">
                                <li><h3>wishlist empty</h3><a href=""></a></li>
                                <li><p>if items in your wishlit are missing, <a href="">login to your account</a> to view them</p></li>
                            </ul>
                        </li>
                    </ul>
                    <div class="search">
                        @using (Html.BeginForm("Search", "Offer", FormMethod.Get, new { enctype = "multipart/form-data" }))
                        {
                            <input type="text" name="mainSearch" id="mainSearch" placeholder="search..." autofocus>
                            <input type="text" name="mainSearchCounties" id="mainSearchCounties" placeholder="Toata Romania">
                            <input type="submit" value="Cauta acum">
                        }
                        @*div for Main Search preview*@
                        <div id="MainSearchPreview"></div>

                        @*div for Main Search Counties preview*@
                        <div id="MainSearchCountiesPreview"></div>

                        @*div for Add To Cart preview*@
                        <div id="AddToCartPreview" title="Produsul a fost adaugat in cos"></div>

                    </div>
                    <div class="clearfix"> </div>
                </div>
                <!-- start header menu -->
                <ul class="megamenu skyblue">
                    <li><a class="color1" href="index.html">Home</a></li>
                    <li class="grid">
                        <a class="color2" href="#">Fructe</a>
                        <div class="megapanel">
                            <div class="row">
                                <div class="col1">
                                    <div class="h_nav">
                                        <h4>Fructe de pădure</h4>
                                        <ul>
                                            <li><a href="women.html">Afină</a></li>
                                            <li><a href="women.html">Corcodușe</a></li>
                                            <li><a href="women.html">Fragi</a></li>
                                            <li><a href="women.html">Mure</a></li>
                                            <li><a href="women.html">Zmeură</a></li>
                                            <li><a href="women.html">Măceșe</a></li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="col1">
                                    <div class="h_nav">
                                        <h4>Fructe de casă</h4>
                                        <ul>
                                            <li><a href="women.html">Mere</a></li>
                                            <li><a href="women.html">Pere</a></li>
                                            <li><a href="women.html">Prune</a></li>
                                            <li><a href="women.html">Cireșe</a></li>
                                            <li><a href="women.html">Vișine</a></li>
                                            <li><a href="women.html">Caise</a></li>
                                            <li><a href="women.html">Piersici</a></li>
                                            <li><a href="women.html">Struguri</a></li>
                                            <li><a href="women.html">Struguri de masă</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col2"></div>
                                <div class="col1"></div>
                                <div class="col1"></div>
                                <div class="col1"></div>
                                <div class="col1"></div>
                            </div>
                        </div>
                    </li>
                    <li class="active grid">
                        <a class="color4" href="#">Legume</a>
                        <div class="megapanel">
                            <div class="row">
                                <div class="col1">
                                    <div class="h_nav">
                                        <h4>legume</h4>
                                        <ul>
                                            <li><a href="women.html">Cartofi</a></li>
                                            <li><a href="women.html">Roșii</a></li>
                                            <li><a href="women.html">Castraveți</a></li>
                                            <li><a href="women.html">Morcovi</a></li>
                                            <li><a href="women.html">Ceapă</a></li>
                                            <li><a href="women.html">Ardei</a></li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="col1">
                                    <div class="h_nav">
                                        <h4>legume</h4>
                                        <ul>
                                            <li><a href="women.html">Varză</a></li>
                                            <li><a href="women.html">Sfeclă</a></li>
                                            <li><a href="women.html">Părtunjel</a></li>
                                            <li><a href="women.html">Țelină</a></li>
                                            <li><a href="women.html">Praz</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col2"></div>
                                <div class="col1"></div>
                                <div class="col1"></div>
                                <div class="col1"></div>
                                <div class="col1"></div>
                            </div>
                        </div>
                    </li>
                    <li>
                        <a class="color6" href="#">Bauturi de casă</a>
                        <div class="megapanel">
                            <div class="row">
                                <div class="col1">
                                    <div class="h_nav">
                                        <h4>legume</h4>
                                        <ul>
                                            <li><a href="women.html">Țuică</a></li>
                                            <li><a href="women.html">Vișinată</a></li>
                                            <li><a href="women.html">Cireșată</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col2"></div>
                                <div class="col1"></div>
                                <div class="col1"></div>
                                <div class="col1"></div>
                                <div class="col1"></div>
                            </div>
                        </div>
                    </li>
                    <li>
                        <a class="color7" href="#">Handmade</a>
                    </li>
                    <li>
                        <a class="color8" href="#">Lapte- Miere- Dulcețuri</a>
                    </li>
                    <li>
                        <a class="color9" href="#">Preparate</a>
                    </li>
                    <li>
                        <a class="color9" href="#">Cărnuri- Brânzeturi</a>

                    </li>
                    <li>
                        <a class="color10" href="contact.html">Contact</a>
                        <div class="megapanel">
                            <div class="row">
                                <div class="col3">
                                    <div class="h_nav">
                                        <h4>contact us</h4>
                                    </div>
                                    <form class="contact">
                                        <label for="name">Name</label>
                                        <input id="name" type="text" />
                                        <label for="email">E-mail</label>
                                        <input id="email" type="text" />
                                        <label for="message">Message</label>
                                        <textarea rows="8" id="message"></textarea>
                                        <input type="submit" value="Send" />
                                    </form>
                                </div>
                                <div class="col3">
                                </div>
                            </div>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>
    @Scripts.Render("~/bundles/jquery")
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/menu")
    @RenderSection("scripts", required: false)
    <!--webfont-->
    <script>$(document).ready(function () { $(".megamenu").megamenu(); });</script>

    <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>
</body>
</html>